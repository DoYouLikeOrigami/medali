	<section class="contacts-section" id="contacts">
		<div class="contacts-section__wrap">
			<div class="contacts-section__left">
				<header class="contacts-section__header">Наши контакты</header>
				<ul class="contacts-section__list">
					<li class="contacts-section__item">
						<div class="contacts-section__img"><img src="./static/img/contacts-section/icons/icon-mail.png"></div>
						<div class="contacts-section__info">
							<header>Пишите нам</header>
							<p><a href="mailto:support@jokerinteractive.ru">support@jokerinteractive.ru</a></p>
						</div>
					</li>
					<li class="contacts-section__item">
						<div class="contacts-section__img"><img src="./static/img/contacts-section/icons/icon-tel.png"></div>
						<div class="contacts-section__info">
							<header>Звоните нам</header>
							<p><a href="tel:+79999999999">+7 (999) 999-99-99</a></p>
						</div>
					</li>
				</ul>
			</div>
			<div class="contacts-section__right">
				<div class="contacts-section__header">Напишите нам</div>
				<form class="form" method="post" action="/send-mail">
					<label class="form__label">
						<input class="form__input" type="email" required placeholder="Ваш e-mail">
					</label>
					<label class="form__label">
						<input class="form__input" type="text" required placeholder="Ваше имя">
					</label>
					<textarea class="form__textarea" placeholder="Ваше сообщение" rows="7"></textarea>
					<input class="btn btn--main" type="submit" value="Написать сообщение">
				</form>
			</div>
		</div>
	</section>
